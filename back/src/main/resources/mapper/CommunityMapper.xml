<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rrrent.theia.dao.CommunityDao">

    <insert id="addCommunity" parameterType="com.alibaba.fastjson.JSONObject">
        INSERT INTO `community`
        (`name`, `architecture_age`, `architecture_type`, `architecture_sum`, `house_sum`, `service_company`, `service_fee`, `develop_company`, `address`, `second_hand_price`)
        VALUES
        (#{name}, #{architectureAge}, #{architectureType}, #{architectureSum}, #{houseSum}, #{serviceCompany}, #{serviceFee}, #{developCompany}, #{address}, #{secondHandPrice});
    </insert>



</mapper>